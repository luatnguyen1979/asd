<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="asd.day10.lab12_2.ClientApp">
	<rowConstraints>
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
		<RowConstraints maxHeight="33.0" minHeight="10.0"
			prefHeight="29.0" />
		<RowConstraints maxHeight="31.0" minHeight="10.0"
			prefHeight="31.0" />
		<RowConstraints minHeight="10.0" prefHeight="30.0" />
	</rowConstraints>
	<columnConstraints>
		<ColumnConstraints maxWidth="96.0" minWidth="10.0"
			prefWidth="44.0" />
		<ColumnConstraints maxWidth="166.0" minWidth="10.0"
			prefWidth="139.0" />
		<ColumnConstraints maxWidth="162.0" minWidth="10.0"
			prefWidth="117.0" />
		<ColumnConstraints maxWidth="162.0" minWidth="10.0"
			prefWidth="117.0" />
	</columnConstraints>
	<children>
		<TextField fx:id="tfName" prefHeight="27.0" prefWidth="106.0"
			GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<Label text="Name: " GridPane.rowIndex="1" />
		<Button mnemonicParsing="false" onAction="#handleAddButtonAction"
			text="Add" GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<Label alignment="CENTER" prefHeight="17.0" prefWidth="233.0"
			text="Person List" GridPane.columnIndex="2" />
		<ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0"
			GridPane.columnIndex="2"  GridPane.rowIndex="1"
			GridPane.rowSpan="3" />
		<Button alignment="CENTER" mnemonicParsing="false"
			onAction="#handleRemoveButtonAction" prefHeight="27.0" prefWidth="243.0"
			text="Remove" GridPane.columnIndex="2" 
			GridPane.rowIndex="4" />
	</children>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</GridPane>
