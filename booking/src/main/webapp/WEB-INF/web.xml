<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>asd.booking.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterCustomerServlet</servlet-name>
    <servlet-class>asd.booking.RegisterCustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterCustomerServlet</servlet-name>
    <url-pattern>/registercustomer</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SearchSchedule</display-name>
    <servlet-name>SearchSchedule</servlet-name>
    <servlet-class>asd.booking.SearchSchedule</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchSchedule</servlet-name>
    <url-pattern>/searchschedule</url-pattern>
  </servlet-mapping>
</web-app>